<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 配置mapper，由于使用spring和mybatis整合来进行mapper扫描，所以不需要配置，必须遵循mapper.xml文件
		和mapper文件同名，并且放置在同一个目录下-->
	<!-- 配置别名 -->
	<typeAliases>
		<!-- 批量扫描别名 -->
		<package name="com.cauchy.ssm.po"/>
	</typeAliases>
</configuration>